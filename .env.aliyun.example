# 阿里云配置示例
# 复制此文件为 .env.local 并填写实际值

# 选择数据库提供商
VITE_USE_ALIYUN_DB=true

# 阿里云RDS PostgreSQL配置
VITE_ALIYUN_DB_HOST=your-rds-instance.pg.rds.aliyuncs.com
VITE_ALIYUN_DB_PORT=5432
VITE_ALIYUN_DB_NAME=ai_learning
VITE_ALIYUN_DB_USER=your_username
VITE_ALIYUN_DB_PASSWORD=your_password
VITE_ALIYUN_DB_SSL=true

# JWT密钥（用于用户认证）
VITE_JWT_SECRET=your-secret-key-please-change-in-production

# DeepSeek API（AI对话功能）
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key

# 阿里云OSS配置（文件存储）
VITE_OSS_REGION=oss-cn-shenzhen
VITE_OSS_ACCESS_KEY_ID=your_access_key_id
VITE_OSS_ACCESS_KEY_SECRET=your_access_key_secret
VITE_OSS_BUCKET=your_bucket_name

# Supabase配置（如果不使用阿里云时的备选）
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key